{
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/*.spec.{js,jsx}"
    ],
    "coverageDirectory": "./coverage/",
    "errorOnDeprecated": true,
    "globals": {},
    "verbose": true
  },
  "devDependencies": {
    "jest": "^24.0.0"
  }
}
